{
  "vault_url": "https://YOUR_VAULT_ID.vault.skyflowapis.com",
  "bearer_token": "YOUR_SKYFLOW_API_KEY_BEARER_TOKEN",
  "batch_size": 100,
  "max_concurrency": 50,
  "max_retries": 3,
  "retry_delay_ms": 500,
  "data_type_mappings": {
    "NAME": {
      "vault_id": "YOUR_VAULT_ID_FOR_NAMES",
      "table": "persons",
      "column": "name"
    },
    "ID": {
      "vault_id": "YOUR_VAULT_ID_FOR_IDS",
      "table": "persons",
      "column": "person_id"
    },
    "DOB": {
      "vault_id": "YOUR_VAULT_ID_FOR_DOBS",
      "table": "persons",
      "column": "date_of_birth"
    },
    "SSN": {
      "vault_id": "YOUR_VAULT_ID_FOR_SSNS",
      "table": "persons",
      "column": "ssn"
    }
  }
}
